<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="E2EDemoRunner" preserve-order="true"  skipfailedinvocationcounts="true" configfailurepolicy="continue" thread-count="2" parallel="false">
 <!--  <parameter name="browsertype" value="firefox"></parameter>  -->
  <listeners>
	  <listener class-name="com.hp.utility.RetryListener" />
  </listeners>
  
  
 <!-- login in the home page -->
 <test enabled="false" name="login in the home page" preserve-order="true" skipfailedinvocationcounts="true" >
   <classes>
     <class name="com.hp.tests.LoginHomeTest"></class>   
   </classes>
  </test>
 
  <!-- new a Proactive Scan Assessment -->
 
  <test enabled="false" name="New FWSW Assessment and search run result" preserve-order="true" skipfailedinvocationcounts="true">
   <classes>
    <class name="com.hp.tests.NewProactiveScanViaSNPNTest"></class> 
   </classes>
  </test>

  <!-- send today's execution report email -->
   
  <test enabled="true" name="PAF Today Execution Email Report" preserve-order="true" skipfailedinvocationcounts="true">
    <classes>
      <class name="com.hp.tests.ExecutionEmailTest"></class>
    </classes>
  </test>  

  <!-- send the overview execution report email -->
 
  <test enabled="false" name="PAF Automation Status Overview Email Report" preserve-order="true" skipfailedinvocationcounts="true">
    <classes>
      <class name="com.hp.tests.OverviewEmailTest"></class>
    </classes>
  </test>  
</suite> <!-- Suite -->
